<extendslayout></extendslayout>
<h2>Log In</h2>
<p class="lead">Log in to existing account
  <form action="/account/register" method="post" class="form-horizontal">
    <fieldset><br/>
      <div class="control-group">
        <div class="control-group">
          <label for="name.first" class="control-label">First Name </label>
          <div class="controls">
            <input type="text" name="name.first" placeholder="First Name"/>
          </div>
        </div>
        <div class="control-group">
          <label for="name.last" class="control-label">Last Name</label>
          <div class="controls">
            <input type="text" name="name.last" placeholder="Last Name"/>
          </div>
        </div>
      </div>
      <div class="control-group">
        <div class="control-group">
          <label for="email" class="control-label">Email</label>
          <div class="controls">
            <input type="text" name="email" placeholder="Email Address"/>
          </div>
        </div>
      </div>
      <div class="control-group">
        <div class="control-group">
          <label for="password" class="control-label">Password</label>
          <div class="controls">
            <input type="password" name="password" placeholder="Password"/>
          </div>
        </div>
        <div class="control-group">
          <label for="passwordConf" class="control-label">Password Confirm</label>
          <div class="controls">
            <input type="password" name="passwordConf" placeholder="Password Confirm"/>
          </div>
        </div>
      </div>
    </fieldset>
  </form>
</p>